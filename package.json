    "@tsconfig/recommended": "^1.0.1",
    "@types/base64id": "^2.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/jest": "^27.4.0",
    "@types/jquery": "^3.5.13",
    "@types/jwplayer": "^8.2.10",
    "@types/lodash": "^4.14.178",
