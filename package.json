{
  "name": "anime-watch-parties",
  "version": "0.4.5",
  "description": "A cross-platform extension that synchronizes videos on major anime websites.",
  "private": true,
  "scripts": {
    "start": "ts-node --project src/server/tsconfig.json src/server/server.ts",
    "debug": "DEBUG=*erverAWP:* npm start",
    "test": "jest",
    "release": "webpack --mode=production && mkdirp built/Release/Firefox/ && rimraf built/Release/Firefox/Anime-Watch-Parties.xpi && jszip-cli add --noconfig --force -o built/Release/Firefox/Anime-Watch-Parties.xpi built/Firefox/src built/Firefox/LICENSE built/Firefox/manifest.json",
    "build-extension": "webpack --mode=development",
    "watch-extension": "webpack --mode=development -w",
    "cert": "openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -days 365 -nodes -subj \"/CN=localhost\" -sha256"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dragicafit/Anime-Watch-Parties.git"
  },
  "author": "Dragicafit",
  "license": "MIT",
  "dependencies": {
    "@socket.io/redis-adapter": "^7.1.0",
    "clipboard": "^2.0.10",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
