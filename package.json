    "express-handlebars": "^6.0.2",
    "lodash": "^4.17.21",
    "rate-limiter-flexible": "^2.3.6",
    "redis": "^3.1.2",
    "socket.io": "^4.4.1"
  },
  "bugs": {
    "url": "https://github.com/Dragicafit/Anime-Watch-Parties/issues"
  },
  "homepage": "https://github.com/Dragicafit/Anime-Watch-Parties#readme",
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.1.6",
    "@primer/css": "19.1.1",
